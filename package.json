{
  "name": "cameotest",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
  "build": "pnpm -r run build",
  "dev": "concurrently -k -n shared,server,client \"pnpm --filter @cameotest/shared dev\" \"pnpm --filter @cameotest/server dev\" \"pnpm --filter @cameotest/client dev\"",
  "start": "pnpm --filter @cameotest/server start",
  "lint": "pnpm -r run lint"
  }
}
