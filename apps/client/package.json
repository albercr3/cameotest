{
  "name": "@cameotest/client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "pnpm install --filter @cameotest/shared --filter @cameotest/magicgrid --filter @cameotest/client --ignore-scripts --prefer-offline && pnpm --filter @cameotest/shared build && pnpm --filter @cameotest/magicgrid build",
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "pnpm --filter @cameotest/shared build && pnpm --filter @cameotest/magicgrid build && ../server/node_modules/.bin/tsx --test --test-concurrency=1 src/**/*.test.tsx"
  },
  "dependencies": {
    "@cameotest/shared": "workspace:*",
    "@cameotest/magicgrid": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "jsdom": "^24.1.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0"
  }
}
